  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "5.5.0",
    "get-port": "4.0.0",
    "jsdoc-api": "4.0.3",
    "jsdoc-parse": "3.0.1",
    "mocha": "5.2.0",
    "node-fetch": "2.2.0",
    "puppeteer": "1.8.0",
    "sauce-test-runner": "0.2.0",
    "sinon": "6.2.0",
    "static-server": "2.2.1",
    "typescript": "3.0.3"
  },
  "scripts": {
    "buildDocs": "node tasks/buildDocs.js",
    "buildWeekData": "node tasks/buildWeekData.js",
    "ci": "npm run lint && npm run sauce-tests",
    "ci:fast": "npm run lint && npm run test -- --run-in-ci",
    "lint": "eslint .; tsc",
    "sauce-tests": "node tasks/sauce-tests.js",
