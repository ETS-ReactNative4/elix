  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "target": "ES6"
  },
  "include": [
    "elix.js",
    "demos/demos.js",
    "tests/tests.js"
  ],
  "exclude": [
    "elements/boilerplate/*"
  ]
}
